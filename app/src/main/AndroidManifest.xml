<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.donnu.organaize">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Organaize">
        <activity android:name=".calendar.EditCalendarActivity"></activity>
        <activity android:name=".calendar.NewCalendarActivity" />
        <activity
            android:name=".calendar.CalendarActivity"
            android:label="@string/title_activity_calendar"
            android:theme="@style/Theme.Organaize.NoActionBar" />
        <activity
            android:name=".notes.NotesActivity"
            android:label="@string/title_activity_notes"
            android:theme="@style/Theme.Organaize.NoActionBar" />
        <activity android:name=".notes.EditNotesActivity" />
        <activity android:name=".notes.NewNotesActivity" />
        <activity android:name=".telephone.NewTelephone" />
        <activity android:name=".telephone.EditTelephone" />
        <activity
            android:name=".telephone.TelephoneActivity"
            android:label="@string/title_activity_telephone"
            android:theme="@style/Theme.Organaize.NoActionBar" />
        <activity
            android:name=".main.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Organaize.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>